# ================================
# DATABASE CONFIGURATION
# ================================
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=tick

# ================================
# JWT CONFIGURATION
# ================================
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h
TOKEN_SECRET=your_token_secret_here
JWT_REFRESH_SECRET=your_refresh_secret_here

# ================================
# SUPABASE CONFIGURATION
# ================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key_here

# ================================
# APIFY CONFIGURATION
# ================================
APIFY_TOKEN=apify_api_your_token_here
USERNAME=your_username

# ================================
# GOOGLE CLOUD / OAUTH CONFIGURATION
# ================================
clientid=your_google_oauth_client_id.apps.googleusercontent.com

# ================================
# GOOGLE AI & CLOUD CONFIGURATION
# ================================
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_AI_API_KEY=your_google_ai_api_key
GOOGLE_CLOUD_TYPE=service_account
GOOGLE_CLOUD_PRIVATE_KEY_ID=your_private_key_id
GOOGLE_CLOUD_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
GOOGLE_CLOUD_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_CLOUD_CLIENT_ID=your_client_id
GOOGLE_CLOUD_AUTH_URI=https://accounts.google.com/o/oauth2/auth
GOOGLE_CLOUD_TOKEN_URI=https://oauth2.googleapis.com/token
GOOGLE_CLOUD_AUTH_PROVIDER_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
GOOGLE_CLOUD_CLIENT_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com

# ================================
# STORAGE CONFIGURATION
# ================================
DEFAULT_STORAGE_PROVIDER=supabase  # Options: 'supabase' or 'google_storage'

# ================================
# APPLICATION CONFIGURATION
# ================================
NODE_ENV=development  # Options: 'development', 'production', 'test'
PORT=3000
